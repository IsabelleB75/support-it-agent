# Copier ce fichier en .env et remplir les valeurs

# Airflow secrets
# Generer Fernet Key: python -c "from cryptography.fernet import Fernet; print(Fernet.generate_key().decode())"
AIRFLOW_FERNET_KEY=your_fernet_key_here
AIRFLOW_SECRET_KEY=your_secret_key_here

# API Mistral
MISTRAL_API_KEY=your_mistral_api_key_here

# PostgreSQL
POSTGRES_HOST=localhost
POSTGRES_PORT=5433

# AWS pour DVC (optionnel, pour dvc pull)
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key

# GitHub Token (pour declenchement CI/CD automatique apres retraining)
GITHUB_TOKEN=ghp_your_github_token
